  "vim.useSystemClipboard": true,
  // 由vim接管ctrl+any的按键，而不是vscode
  "vim.useCtrlKeys": true,
  // 突出显示与当前搜索匹配的所有文本
  "vim.hlsearch": true,
  "vim.leader": "<space>",
  "vim.flash.enable": true,
  "vim.digraphs": {},
  // 忽略大小写
  "vim.ignorecase": true,
  "vim.smartcase": true,
  // 在折叠状态下上下移动不会自动展开(开启后光标快速移动时会出现卡顿)
  "vim.foldfix": false,
