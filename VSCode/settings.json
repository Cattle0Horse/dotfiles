  "markdown-preview-enhanced.breakOnSingleNewLine": false, // 是否单换行符触发新段落（Github风格是单换行符视为空格，末尾两空格或两个换行符视为新段落）
  "markdown-preview-enhanced.enableEmojiSyntax": true, // 启用表情符号语法（如 :smile:），仅适用于 markdown-it 解析器，不适用于pandoc
  "markdown-preview-enhanced.enableWikiLinkSyntax": true, // 启用维基链接语法（如 [[Page]]）
