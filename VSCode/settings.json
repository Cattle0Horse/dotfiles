  "markdown-preview-enhanced.jsdelivrCdnHost": "cdn.jsdelivr.net",
  "markdown-preview-enhanced.mathjaxV3ScriptSrc": "https://cdn.jsdelivr.net/npm/mathjax@3/es5/tex-mml-chtml.js", // MathJax v3脚本地址
  "markdown-preview-enhanced.mathRenderingOnlineService": "https://latex.codecogs.com/gif.latex", // 在线LaTeX渲染服务
  // === HTML与多媒体 ====================================================
