  // "vim.autoSwitchInputMethod.enable": true,
  // "vim.autoSwitchInputMethod.defaultIM": "1033",
  // "vim.autoSwitchInputMethod.obtainIMCmd": "path/to/im-select.exe", // HINT: change this
  // "vim.autoSwitchInputMethod.switchIMCmd": "path/to/im-select.exe {im}", // HINT: change this
  /* 
        note: 
        gcc: 行注释
        gCC: 块注释
        gd: 跳转至定义(函数内)，配合返回 <alt+方向键> 来回跳转
        gD: 跳转至定义(文件内)，配合返回 <alt+方向键> 来回跳转
      */
  "vim.normalModeKeyBindingsNonRecursive": [
    // 更换为 i j k l 为 上 左 下 右
    { "before": ["i"], "after": ["k"] },
    { "before": ["k"], "after": ["j"] },
    { "before": ["j"], "after": ["h"] },
    { "before": ["J"], "after": ["^"] },
    { "before": ["L"], "after": ["$"] },
    { "before": ["I"], "after": ["5", "k"] },
    { "before": ["K"], "after": ["5", "j"] },
    // h选中后续字符中间的内容, a选中后续字符及中间内容(就像"abc",h会选择abc,a会选择"abc")
    // 光标到屏幕上方(移动屏幕)
    { "before": ["z", "i"], "after": ["z", "t"] },
    // 光标到屏幕下方(移动屏幕)
    { "before": ["z", "k"], "after": ["z", "b"] },
    // 切换折叠
    { "before": ["z", "f"], "commands": ["editor.toggleFold"], "silent": true },
    // h插入模式
    { "before": ["h"], "after": ["i"] },
    // H行首插入模式(不含空白字符)
    { "before": ["H"], "after": ["I"] },
    // undo
    { "before": ["u"], "commands": ["undo"] },
    // redo
    { "before": ["<C-r>"], "commands": ["redo"] },
    // tab切换(gt/gT)
    { "before": ["<C-j>"], "commands": [":tabprev"], "silent": true },
    { "before": ["<C-l>"], "commands": [":tabnext"], "silent": true },
    // 上一个/下一个问题
    { "before": ["g", "["], "commands": ["editor.action.marker.prevInFiles"] },
    { "before": ["g", "]"], "commands": ["editor.action.marker.nextInFiles"] },
    // 取消搜索高亮
    { "before": ["<leader>", "n"], "commands": [":nohl"], "silent": true },
    // whichkey
    { "before": [":"], "commands": ["whichkey.show"], "silent": true },
    // 合并行
    { "before": ["Q"], "commands": ["editor.action.joinLines"], "silent": true },
    // 绑定<leader>w保存当前文件：
    { "before": ["<leader>", "w"], "commands": ["workbench.action.files.save"], "silent": true },
    // toggle boolean
    { "before": ["<leader>", "b"], "commands": ["extension.toggleBool"], "silent": true },
    // 打开命令面板
    { "before": ["<leader>", "o", "p"], "commands": ["workbench.action.showCommands"], "silent": true },
    // 打开面板
    { "before": ["<leader>", "o", "f"], "commands": ["workbench.action.quickOpen"], "silent": true },
    // 向下复制行(或shift+alt+↓)(shift+alt+ctrl+↓向下添加光标)
    { "before": ["<leader>", "c", "k"], "commands": ["editor.action.copyLinesDownAction"], "silent": true },
    // 向上复制行(或shift+alt+↑)(shift+alt+ctrl+↑向上添加光标)
    { "before": ["<leader>", "c", "i"], "commands": ["editor.action.copyLinesUpAction"], "silent": true },
    // code runner
    { "before": ["<leader>", "c", "r"], "commands": ["code-runner.run"], "silent": true },
    // format document 格式化
    { "before": ["<leader>", "f", "d"], "commands": ["editor.action.formatDocument"], "silent": true },
    // rename 重命名变量
    { "before": ["<leader>", "r", "n"], "commands": ["editor.action.rename"], "silent": true },
    // 跳转到上一个错误
    { "before": ["<leader>", "g", "i"], "commands": ["editor.action.marker.prev"], "silent": true },
    // 跳转到下一个错误
    { "before": ["<leader>", "g", "k"], "commands": ["editor.action.marker.next"], "silent": true },
    // 左移tab
    { "before": ["<leader>", "<", "<"], "commands": ["workbench.action.moveEditorLeftInGroup"], "silent": true },
    // 右移tab
    { "before": ["<leader>", ">", ">"], "commands": ["workbench.action.moveEditorRightInGroup"], "silent": true },
    // 不要将删除内容放入剪贴板
    { "before": ["<delete>"], "after": ["\"", "_", "delete"] }
  ],
