  "markdown-preview-enhanced.HTML5EmbedAudioAttributes": "controls preload=\"metadata\" width=\"320\"", // HTML5音频标签默认属性
  "markdown-preview-enhanced.HTML5EmbedVideoAttributes": "controls preload=\"metadata\" width=\"320\" height=\"240\"", // HTML5视频标签默认属性
  "markdown-preview-enhanced.HTML5EmbedUseImageSyntax": true, // 使用Markdown图片语法替代HTML图片标签
  "markdown-preview-enhanced.HTML5EmbedIsAllowedHttp": false, // 禁用HTTP协议资源嵌入
  "markdown-preview-enhanced.HTML5EmbedUseLinkSyntax": false, // 允许使用链接语法嵌入HTML5（如 <audio src="audio.mp3">）
  // === 工具与服务集成 ==================================================
