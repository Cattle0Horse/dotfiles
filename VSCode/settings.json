    { "before": ["z", "i"], "after": ["z", "t"] },
    // 光标到屏幕下方(移动屏幕)
    { "before": ["z", "k"], "after": ["z", "b"] },
    // 显示悬停
    { "before": ["g", "h"], "commands": ["editor.action.showHover"], "silent": true },
    // whichkey
    { "before": [":"], "commands": ["whichkey.show"], "silent": true },
    // 合并行
    { "before": ["Q"], "commands": ["editor.action.joinLines"], "silent": true },
    // 向下复制行
    { "before": ["<leader>", "c", "k"], "commands": ["editor.action.copyLinesDownAction"], "silent": true },
    // 向上复制行
    { "before": ["<leader>", "c", "i"], "commands": ["editor.action.copyLinesUpAction"], "silent": true },
    // 不要将删除内容放入剪贴板
    { "before": ["delete"], "after": ["\"", "_", "delete"] }
  ],
  "vim.operatorPendingModeKeyBindingsNonRecursive": [
    // 更换为 i j k l 为 上 左 下 右
    { "before": ["i"], "after": ["k"] },
    { "before": ["k"], "after": ["j"] },
    { "before": ["j"], "after": ["h"] },
    { "before": ["J"], "after": ["^"] },
    { "before": ["L"], "after": ["$"] },
    { "before": ["h"], "after": ["i"] },
    // 不要将删除内容放入剪贴板
    { "before": ["delete"], "after": ["\"", "_", "delete"] }
  ],
