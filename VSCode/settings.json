{
  // toggleSuggestionDetails 按键切换用户代码片段预览(建议细节, 及其代码片段过大及其卡顿, 可查看按键绑定切换)
  // ----------------- 系统设置 -----------------
  // 文件显示过滤
  "files.exclude": {
    // 版本控制相关
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    // 操作系统元文件
    "**/.DS_Store": true,
    "**/Thumbs.db": true
  },
  // 控制 UI 是否应在已针对屏幕阅读器进行优化的模式下运行
  "editor.accessibilitySupport": "auto",
  // 保存文件时，在文件末尾添加单独的空行（如果没有）
  "notebook.insertFinalNewline": true,
  // 关闭copilot
  "chat.commandCenter.enabled": false,
  "github.copilot.enable": {
    "*": false
  },
  // 控制在粘贴本机文件和文件夹时资源管理器是否应要求进行确认
  "explorer.confirmPasteNative": false,
  // 保存文件时在文件末尾插入一个最终新行
  "files.insertFinalNewline": true,
  // 禁用遥测
  "telemetry.telemetryLevel": "off",
  "telemetry.feedback.enabled": false,
  // 禁用遥测
  "redhat.telemetry.enabled": false,
  // vscode内置文件浏览器替代系统浏览器
  "files.simpleDialog.enable": true,
  // 正斜杠路径复制
  "explorer.copyRelativePathSeparator": "/",
  // 关闭烦人的Workspace Trust提示框
  "security.workspace.trust.enabled": false,
  // 差异编辑器忽略前导空格或尾随空格中的更改
  "diffEditor.ignoreTrimWhitespace": false,
  // 编辑器行高
  "editor.lineHeight": 1.4,
  // chinese
  "editor.unicodeHighlight.allowedLocales": { "zh-hans": true, "zh-hant": true },
  // 顶部窗口显示标题
  "window.title": "${dirty}${folderName}${separator}${activeEditorShort}",
  // 字体大小设置
  "editor.fontSize": 17,
  // "editor.fontFamily": "'Cascadia Code NF', '等距更纱黑体 SC', monospace",
  "editor.fontFamily": "'Maple Mono NF CN'",
  // 字体样式设置
  "editor.fontLigatures": "'calt', 'cv01', 'cv03', 'cv35', 'ss01', 'ss06'",
  // 字重
  "editor.fontWeight": "normal",
  // 启用括号指导线
  "editor.guides.bracketPairs": true,
  // 启用高亮选中的缩进指导线
  "editor.guides.highlightActiveIndentation": true,
  // 启用水平括号指导线
  "editor.guides.bracketPairsHorizontal": "active",
  // 启用缩进指导线
  "editor.guides.indentation": true,
  // 启用括号着色
  "editor.bracketPairColorization.enabled": true,
  // 资源管理器缩进大小
  "workbench.tree.indent": 16,
  // 键入;时格式化
  "editor.formatOnType": false,
  // 编辑器默认格式化程序
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  // 粘贴时不格式化文件
  "editor.formatOnPaste": false,
  // 保存时不格式化文件
  "editor.formatOnSave": false,
  // 在基于文件内容打开文件时是否自动检测 #editor.tabSize# 和 #editor.insertSpaces#
  "editor.detectIndentation": true,
  // 一个制表符几个空格（此处的值仅为默认值，会被自动检测覆盖）
  "editor.tabSize": 2,
  // 同上，自动检测后会以文件实际情况为准
  "editor.insertSpaces": true,
  // 控制是否及如何在“导航路径”视图中显示文件路径(tab栏下方)
  "breadcrumbs.filePath": "on",
  // 编辑器始终自动改写左引号或右引号
  "editor.autoClosingOvertype": "always",
  // 编辑器始终自动删除右引号和右方括号
  "editor.autoClosingDelete": "always",
  // 在编辑器中显示内联提示
  "editor.inlayHints.enabled": "on",
  // 在编辑器中自动显示内联建议
  "editor.inlineSuggest.enabled": true,
  // 显示片段建议
  "editor.suggest.showSnippets": true,
  // 输入时显示含有参数文档和类型信息的小面板
  "editor.parameterHints.enabled": true,
  // scroll bar
  "editor.scrollbar.vertical": "auto",
  "editor.scrollbar.horizontal": "auto",
  // 忽略扩展推荐
  "extensions.ignoreRecommendations": true,
  "extensions.autoUpdate": "onlyEnabledExtensions",
  // 控制是否在键入代码时自动显示建议
  "editor.quickSuggestions": { "comments": true, "other": true, "strings": true },
  // 不以紧凑形式显示文件夹
  "explorer.compactFolders": false,
  //smart: 在重复名称末尾智能地添加/递增数字
  "explorer.incrementalNaming": "smart",
  // 控制显示快速建议前的等待时间（毫秒）
  "editor.quickSuggestionsDelay": 0,
  // 启用粘滞滚动，即显示上一级对应的代码
  "editor.stickyScroll.enabled": true,
  // 使用空格缩进时模拟制表符的行为，可以方便对齐
  "editor.stickyTabStops": true,
  // zen 模式显示行号
  "zenMode.hideLineNumbers": false,
  // 折行
  "editor.wordWrap": "on",
  "editor.wordWrapColumn": 120,
  // 建议的接受方式
  "editor.suggest.insertMode": "replace",
  // 控制排序时是否提高靠近光标的词语的优先级
  "editor.suggest.localityBonus": true,
  // 禁用建议必须匹配开头
  "editor.suggest.matchOnWordStartOnly": false,
  // 在多个工作区和窗口间共享建议记忆
  "editor.suggest.shareSuggestSelections": true,
  // 控制建议小部件底部的状态栏可见
  "editor.suggest.showStatusBar": true,
  // 控制在键入触发字符后是否自动显示建议
  "editor.suggestOnTriggerCharacters": true,
  // 始终预先选择第一个建议
  "editor.suggestSelection": "first",
  // 不突出显示可能与基本 ASCII 字符混淆的字符
  "editor.unicodeHighlight.ambiguousCharacters": false,
  // 编辑器相对行号
  "editor.lineNumbers": "relative",
  // 会根据文档中的文字提供建议列表
  "editor.wordBasedSuggestions": "matchingDocuments",
  // 前后<html>标签同时修改
  "editor.linkedEditing": true,
  "editor.rulers": [120],
  // 右侧小地图
  "editor.minimap.enabled": false,
  "editor.minimap.scale": 1,
  "editor.minimap.maxColumn": 120,
  "editor.minimap.renderCharacters": false,
  // 控制单元格编辑器中行号的显示
  "notebook.lineNumbers": "on",
  // 在状态栏显示当前问题
  "problems.showCurrentInStatus": true,
  // 显示搜索结果所在行号
  "search.showLineNumbers": true,
  // 等待一定时间后自动保存
  "files.autoSave": "afterDelay",
  // 自动保存的等待时间（毫秒）
  "files.autoSaveDelay": 60000,
  // 当搜索词为小写时，则不区分大小写进行搜索；否则区分大小写
  "search.smartCase": true,
  // 把文件删除时无需确认
  "explorer.confirmDelete": false,
  // 移动文件时无需确认
  "explorer.confirmDragAndDrop": false,
  // 允许语言检测使用编辑器历史记录
  "workbench.editor.historyBasedLanguageDetection": true,
  // tab栏高度紧凑
  "window.density.editorTabHeight": "compact",
  // 侧边栏按钮位置更换
  "workbench.activityBar.location": "top",
  "window.commandCenter": false,
  "editor.mouseWheelZoom": true,
  "window.restoreWindows": "none",
  "security.workspace.trust.untrustedFiles": "open",
  "workbench.editor.empty.hint": "hidden",
  "workbench.colorTheme": "Eva Dark",
  "editor.cursorBlinking": "solid",
  // 光标平滑移动
  "editor.cursorSmoothCaretAnimation": "on",
  "workbench.list.smoothScrolling": true,
  "workbench.settings.applyToAllProfiles": ["workbench.startupEditor"],
  "workbench.iconTheme": "material-icon-theme",
  "workbench.editor.enablePreview": true,
  // 隐藏关闭按钮
  "workbench.editor.tabActionCloseVisibility": false,
  // 猜测编码
  "files.autoGuessEncoding": true,
  // 语义高亮
  "editor.semanticHighlighting.enabled": true,
  "editor.semanticTokenColorCustomizations": {
    "[Eva Dark]": {
      "enabled": true,
      "rules": {
        // 静态量（静态变量，静态函数）
        "*.static": { "fontStyle": "bold" }
      }
    }
  },
  // 禁用 vscode 声音提示
  "accessibility.signalOptions.volume": 0,
  "accessibility.signals.codeActionTriggered": { "sound": "off" },
  "accessibility.signals.onDebugBreak": { "sound": "off" },
  "accessibility.signals.lineHasBreakpoint": { "sound": "off", "announcement": "off" },
  "accessibility.signals.lineHasError": { "sound": "off", "announcement": "off" },
  "accessibility.signals.lineHasFoldedArea": { "sound": "off", "announcement": "off" },
  "accessibility.signals.lineHasInlineSuggestion": { "sound": "off" },
  "accessibility.signals.lineHasWarning": { "sound": "off", "announcement": "off" },
  "accessibility.signals.chatRequestSent": { "sound": "off" },
  "accessibility.signals.chatResponseReceived": { "sound": "off" },
  "accessibility.signals.clear": { "sound": "off" },
  "accessibility.signals.diffLineDeleted": { "sound": "off" },
  "accessibility.signals.diffLineInserted": { "sound": "off" },
  "accessibility.signals.diffLineModified": { "sound": "off" },
  "accessibility.signals.notebookCellCompleted": { "sound": "off" },
  "accessibility.signals.noInlayHints": { "sound": "off" },
  "accessibility.signals.notebookCellFailed": { "sound": "off" },
  "accessibility.signals.positionHasError": { "sound": "off" },
  "accessibility.signals.progress": { "sound": "off" },
  "accessibility.signals.positionHasWarning": { "sound": "off" },
  "accessibility.signals.taskCompleted": { "sound": "off" },
  "accessibility.signals.taskFailed": { "sound": "off" },
  "accessibility.signals.terminalCommandSucceeded": { "sound": "off" },
  "accessibility.signals.terminalBell": { "sound": "off" },
  "accessibility.signals.terminalQuickFix": { "sound": "off" },
  "accessibility.signals.voiceRecordingStarted": { "sound": "off" },
  "accessibility.signals.voiceRecordingStopped": { "sound": "off" },
  "accessibility.signals.chatEditModifiedFile": { "sound": "off" },
  // ----------------- terminal -----------------
  // "terminal.integrated.fontFamily": "CaskaydiaMono NFM",
  "terminal.integrated.fontFamily": "'Maple Mono NF CN'",
  // 字体样式设置
  "terminal.integrated.fontLigatures.enabled": true,
  "terminal.integrated.fontLigatures.featureSettings": "'calt', 'cv01', 'cv03', 'cv35', 'ss01', 'ss06'",
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.enableMultiLinePasteWarning": "never",
  "terminal.integrated.defaultProfile.windows": "Nushell",
  // 集成终端使用GPU加速
  "terminal.integrated.gpuAcceleration": "on",
  // 集成终端右击时选择光标下方的字词，并打开上下文菜单
  "terminal.integrated.rightClickBehavior": "selectWord",
  "terminal.integrated.shellIntegration.enabled": true,
  // 始终显示“新建终端”按钮旁的“终端拆分”和“终止”按钮
  "terminal.integrated.tabs.showActions": "always",
  // windows 终端设置
  "powershell.integratedConsole.showOnStartup": false,
  "terminal.integrated.profiles.windows": {
    "PowerShell": { "source": "PowerShell", "icon": "terminal-powershell" },
    "Command Prompt": {
      "path": ["${env:windir}\\Sysnative\\cmd.exe", "${env:windir}\\System32\\cmd.exe"],
      "args": [],
      "icon": "terminal-cmd"
    },
    "Nushell": { "path": "nu.exe" }
  },
  "debug.onTaskErrors": "showErrors",
  "debug.console.closeOnEnd": true,

  //  ------------------------ language ----------------------------
  // 文件类型关联
  "files.associations": { "*.clang-format": "yaml", "*.opml": "xml" },
  "[jsonc]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[snippets]": { "editor.defaultFormatter": "vscode.json-language-features" },
  "[xml]": { "editor.defaultFormatter": "redhat.vscode-xml" },
  "[toml]": { "editor.defaultFormatter": "tamasfe.even-better-toml" },
  "[dockerfile]": { "editor.defaultFormatter": "ms-azuretools.vscode-containers" },
  "[properties]": { "editor.defaultFormatter": "foxundermoon.shell-format" },
  "[yaml]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 2,
    "editor.autoIndent": "keep",
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.quickSuggestions": {
      "other": true,
      "comments": false,
      "strings": true
    }
  },
  "[shellscript]": {
    "files.eol": "\n",
    "editor.defaultColorDecorators": "never",
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  // ------------------------ markdown ---------------------------
  "[markdown]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  // ----------------- yzhang.markdown-all-in-one --------------------------
  "markdown.extension.bold.indicator": "**", // ** 粗体语法指示符，默认双星号
  "markdown.extension.completion.enabled": true, // 智能补全（如链接、数学公式等）（使用内置的，因为支持同级目录）
  "markdown.extension.completion.respectVscodeSearchExclude": false, // 补全时是否遵循 VSCode 的搜索排除设置；false 表示补全时也会索引被排除的文件
  "markdown.extension.completion.root": "", // 补全根目录，空字符串表示工作区根
  "markdown.extension.extraLangIds": [], // 额外识别的语言 ID 列表（如 mdx），默认为空
  "markdown.extension.italic.indicator": "*", // * 斜体语法指示符，默认单星号
  "markdown.extension.katex.macros": {}, // KaTeX 自定义宏，可用于数学公式
  "markdown.extension.list.indentationSize": "inherit", // 列表缩进方式，可选 'adaptive'（跟随前一行）或数字，'inherit' 使用当前文档设置的缩进量（请查看 VS Code 状态栏）
  "markdown.extension.list.toggle.candidate-markers": ["-", "*", "+", "1.", "1)"], // 切换列表项时可用的标记符号集合
  "markdown.extension.math.enabled": false, // 启用行内和块级数学公式渲染（渲染被markdown-preview-enhanced替代）
  "markdown.extension.orderedList.autoRenumber": true, // 自动重排有序列表编号
  "markdown.extension.orderedList.marker": "ordered", // 有序列表标记风格，'ordered' 表示使用 1.、2.、…, 'one' 表示使用单一数字
  "markdown.extension.preview.autoShowPreviewToSide": false, // 打开 Markdown 文件时，不自动在侧边打开预览
  "markdown.extension.print.absoluteImgPath": true, // 打印/导出为 HTML 时，将本地图片路径转换为绝对路径
  "markdown.extension.print.imgToBase64": false, // 打印时不将图片编码为 Base64
  "markdown.extension.print.includeVscodeStylesheets": true, // 导出 HTML 时包含 VSCode 默认样式表
  "markdown.extension.print.onFileSave": false, // 保存时不自动触发打印/导出操作
  "markdown.extension.print.pureHtml": false, // 导出包含 VSCode 样式，不是极简纯 HTML
  "markdown.extension.print.theme": "light", // 导出时使用的主题：'light' 或 'dark'
  "markdown.extension.print.validateUrls": true, // 导出前校验所有链接地址有效性
  "markdown.extension.showActionButtons": false, // 在编辑器上方不显示扩展的快捷按钮
  "markdown.extension.syntax.decorationFileSizeLimit": 50000, // 语法高亮装饰的文件大小上限（字节），超过不渲染
  "markdown.extension.syntax.plainTheme": false, // 不使用极简的纯文本主题
  "markdown.extension.tableFormatter.delimiterRowNoPadding": false, // 表格格式化时，分隔行是否无额外内边距
  "markdown.extension.tableFormatter.enabled": true, // 启用表格自动格式化
  "markdown.extension.tableFormatter.normalizeIndentation": false, // 使位于列表内的表格的缩进长度为制表符长度
  "markdown.extension.toc.levels": "1..6", // 目录生成的标题层级范围
  "markdown.extension.toc.omittedFromToc": {}, // 不包含在目录中的标题关键词列表
  "markdown.extension.toc.orderedList": false, // 生成的目录使用无序列表
  "markdown.extension.toc.plaintext": false, // 不仅仅输出纯文本目录，保留链接
  "markdown.extension.toc.slugifyMode": "github", // 目录锚点的 slug 转换模式，模拟 GitHub
  "markdown.extension.toc.unorderedList.marker": "-", // 目录列表项使用的无序列表标记
  "markdown.extension.toc.updateOnSave": true, // 保存时自动更新目录
  // ---------------------- VSCode 内置 ------------------------------
  "markdown.copyFiles.destination": { "**/*": "attachments/${documentBaseName}/" }, // 粘贴本地文件时的目标路径规则
  "markdown.copyFiles.overwriteBehavior": "nameIncrementally", // 文件名冲突时按序号重命名
  "markdown.editor.drop.copyIntoWorkspace": "mediaFiles", // 拖放文件到编辑器时复制到工作区的 mediaFiles 目录
  "markdown.editor.drop.enabled": "smart", // 拖放时智能处理（链接/复制/不处理）
  "markdown.editor.filePaste.audioSnippet": "<audio controls src=\"${src}\" title=\"${title}\"></audio>", // 粘贴音频文件时插入的 HTML 片段模板
  "markdown.editor.filePaste.copyIntoWorkspace": "mediaFiles", // 粘贴文件时复制到 mediaFiles
  "markdown.editor.filePaste.enabled": "smart", // 粘贴时智能处理
  "markdown.editor.filePaste.videoSnippet": "<video controls src=\"${src}\" title=\"${title}\"></video>", // 粘贴视频文件时插入的 HTML 片段模板
  "markdown.editor.pasteUrlAsFormattedLink.enabled": "smartWithSelection", // 当选中文本粘贴 URL 时，智能生成格式化链接
  "markdown.editor.updateLinksOnPaste.enabled": true, // 粘贴文件时自动更新链接
  "markdown.links.openLocation": "currentGroup", // 点击链接在当前编辑组打开
  "markdown.math.enabled": false, // 启用内置数学公式渲染（渲染被markdown-preview-enhanced替代）
  "markdown.math.macros": {}, // 内置数学公式自定义宏
  "markdown.occurrencesHighlight.enabled": false, // 关闭标记同一标记（如链接、标题）时的高亮
  "markdown.preferredMdPathExtensionStyle": "auto", // Markdown 路径扩展名风格自动检测
  "markdown.suggest.paths.enabled": false, // 路径智能补全（markdown all in one 替代）
  "markdown.suggest.paths.includeWorkspaceHeaderCompletions": "onDoubleHash", // 在输入 ## 时提示同目录 Markdown 标题
  "markdown.updateLinksOnFileMove.enabled": "never", // 移动文件时不更新链接
  "markdown.updateLinksOnFileMove.enableForDirectories": true, // 移动目录时更新其下链接
  "markdown.updateLinksOnFileMove.include": [
    // 链接更新范围
    "**/*.{md,mkd,mdwn,mdown,markdown,markdn,mdtxt,mdtext,workbook}",
    "**/*.{jpg,jpe,jpeg,png,bmp,gif,ico,webp,avif,tiff,svg,mp4}"
  ],
  "markdown.validate.enabled": false, // 关闭内置校验（由 markdownlint 替代）
  "markdown.validate.duplicateLinkDefinitions.enabled": "warning", // 重复链接定义时给出警告
  "markdown.validate.fileLinks.enabled": "warning", // 文件链接校验警告
  "markdown.validate.fileLinks.markdownFragmentLinks": "inherit", // 片段链接校验继承全局 fileLinks 设置
  "markdown.validate.fragmentLinks.enabled": "warning", // 内部锚点链接校验警告
  "markdown.validate.ignoredLinks": [], // 忽略的链接校验列表
  "markdown.validate.referenceLinks.enabled": "warning", // 引用式链接校验警告
  "markdown.validate.unusedLinkDefinitions.enabled": "hint", // 未使用的链接定义时给出提示
  // ----------------- shd101wyy.markdown-preview-enhanced -----------------
  // === 核心预览行为与界面 ===============================================
  "markdown-preview-enhanced.configPath": "", // 自定义配置文件路径
  "markdown-preview-enhanced.usePandocParser": false, // 使用 Pandoc 解析器而不是 markdown-it
  "markdown-preview-enhanced.wikiLinkTargetFileExtension": ".md", // wiki链接目标文件扩展名（如[[MyPage.md]]）
  "markdown-preview-enhanced.wikiLinkTargetFileNameChangeCase": "none", // wiki链接目标文件名大小写转换
  "markdown-preview-enhanced.markdownFileExtensions": [".md", ".markdown", ".mdx"], // Markdown文件扩展名
  "markdown-preview-enhanced.enableCriticMarkupSyntax": false, // CriticMarkup语法
  "markdown-preview-enhanced.enableLinkify": true, // 启用链接自动识别
  "markdown-preview-enhanced.liveUpdate": false, // 实时预览更新（编辑时自动刷新，大文件会卡顿）
  "markdown-preview-enhanced.scrollSync": true, // 同步编辑器与预览滚动位置
  "markdown-preview-enhanced.previewMode": "Single Preview", // 预览模式（对于切换markdown文件时）
  "markdown-preview-enhanced.printBackground": false, // 打印时是否包含背景颜色
  "markdown-preview-enhanced.hideDefaultVSCodeMarkdownPreviewButtons": true, // 隐藏VSCode原生预览按钮
  "markdown-preview-enhanced.automaticallyShowPreviewOfMarkdownBeingEdited": false, // 禁用自动显示编辑中的文件预览
  "markdown-preview-enhanced.imageMagickPath": "", // ImageMagick工具路径（图片转换），如果为空，则使用sharp替代
  "markdown-preview-enhanced.chromePath": "", // 指定Chrome浏览器路径
  "markdown-preview-enhanced.frontMatterRenderingOption": "none", // YAML front matter渲染方式
  "markdown-preview-enhanced.alwaysShowBacklinksInPreview": false,
  "markdown-preview-enhanced.revealjsTheme": "black.css",
  "markdown-preview-enhanced.enablePreviewZenMode": false,
  "markdown-preview-enhanced.previewTheme": "one-dark.css",
